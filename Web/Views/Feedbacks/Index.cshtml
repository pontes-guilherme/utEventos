            @Html.DisplayFor(modelItem => item.descricao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.data_criacao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Evento.nome)
        </td>
